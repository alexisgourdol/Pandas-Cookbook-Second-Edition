   "source": [
    "def between_n_m(n, m):\n",
    "    def wrapper(ser):\n",
    "        return pct_between(ser, n, m)\n",
    "\n",
    "    wrapper.__name__ = f\"between_{n}_{m}\"\n",
    "    return wrapper"
   ]
