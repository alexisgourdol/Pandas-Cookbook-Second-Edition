    "def change_col_name(col_name):\n",
    "    col_name = col_name.replace(\"_name\", \"\")\n",
    "    if \"facebook\" in col_name:\n",
    "        fb_idx = col_name.find(\"facebook\")\n",
    "        col_name = col_name[:5] + col_name[fb_idx - 1 :] + col_name[5 : fb_idx - 1]\n",
    "    return col_name"
   ]
